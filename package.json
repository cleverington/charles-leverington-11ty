{
  "name": "eleventy-netlify-boilerplate",
  "version": "1.0.3",
  "description": "A boilerplate for building a fast static website with the Eleventy static site generator, for deployment to Netlify.",
  "scripts": {
    "clean": "rimraf _site",
    "styles:dev": "npx sass src/site/_sass/:src/site/_includes/assets/css --watch --style=expanded",
    "styles:prod": "npx sass src/site/_sass:_site/assets/css --style=compressed --embed-source-map --quiet",
    "serve:eleventy": "ELEVENTY_ENV=development eleventy --serve",
    "serve": "npm-run-all clean --parallel styles:dev serve:eleventy",
    "build": "npm run styles:prod && npx @11ty/eleventy",
    "debug": "DEBUG=* npx @11ty/eleventy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cleverington/charles-leverington-11ty"
  },
  "author": {
    "name": "Charles Leverington",
    "email": "charles.leverington@gmail.com",
    "url": "https://charles-leverington-11ty.netlify.app/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cleverington/charles-leverington-11ty/issues"
  },
  "homepage": "https://github.com/cleverington/charles-leverington-11ty",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0-canary.15",
    "luxon": "^2.3.0",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-checkbox": "^1.1.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-emoji": "^2.0.2",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@11ty/eleventy-navigation": "^0.3.2",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.2",
    "clean-css": "^5.2.4",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.55.0",
    "uglify-js": "^3.15.1"
  }
}
